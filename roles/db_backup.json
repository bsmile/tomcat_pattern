{
  "name": "db_backup",
  "description": "Execute backup process immediately",
  "chef_type": "role",
  "json_class": "Chef::Role",
  "default_attributes": {
    "backup_restore": {
      "sources": {
        "enabled": ["postgresql"]
      }
    }
  },
  "run_list": [
    "backup_restore::backup"
  ]
}
